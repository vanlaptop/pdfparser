{
    "name": "vanlaptop/pdfparser",
    "description": "Pdf parser library fork from Smalot/pdfparser to fit internal templated. Can read and extract information from pdf file.",
    "keywords": [],
    "type": "library",
    "license": "LGPLv3",
    "authors": [
        {
            "name": "Artem Shcherbakov",
            "email": "soft@screencountry.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "ext-iconv": "*",
        "tecnickcom/tcpdf": "~6.0"
    },
    "require-dev": {
        "atoum/atoum": "^2.8 | ^3.0"
    },
    "autoload": {
        "psr-4": {
            "PdfParser\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
          "PdfParser\\": "src"
        }
      },
    "config": {
        "process-timeout": 1200
    }
}
